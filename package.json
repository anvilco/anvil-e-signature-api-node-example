{
  "name": "anvil-signature-api-example",
  "version": "1.0.0",
  "description": "Anvil Etch E-Sign API Example App",
  "main": "src/server/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --port 3001",
    "server": "PORT=8080 nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "yarn eslint 'src/**/*.js' 'test/**/*.js'",
    "test:server": "NODE_ENV=test ANVIL_API_KEY=test mocha 'test/server/**/*.test.js' --require test/server/environment.js",
    "test:client": "NODE_ENV=test mocha 'test/client/**/*.test.js' --require test/client/environment.js --require @babel/register",
    "test": "yarn lint && yarn test:server && yarn test:client"
  },
  "author": "Anvil Foundry Inc.",
  "license": "MIT",
  "repository": "https://github.com/anvilco/anvil-signature-api-example",
  "homepage": "https://github.com/anvilco/anvil-signature-api-example#readme",
  "bugs": {
    "url" : "https://github.com/anvilco/anvil-signature-api-example/issues",
    "email" : "support@useanvil.com"
  },
  "dependencies": {
    "@anvilco/anvil": "^2.3.0",
    "@babel/polyfill": "^7.11.5",
    "app-module-path": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.pickby": "^4.6.0",
    "polished": "^3.6.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-nicenice": "^2.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.2.2",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.5",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
